<h3><%= link_to root_path do %>ロゴ<% end %></h3>
<% if member_signed_in? %>
  <ul>
      <li>ようこそ<%= current_member.nickname %>さん</li>
      <li><%= link_to 'みんなの投稿', posts_path %></li>
      <li><%= link_to '投稿フォーム', new_post_path %></li>
      <li><%= link_to 'マイページ', member_path(current_member) %></li>
      <li>いいねリスト</li>
      <li><%= link_to 'ログアウト', destroy_member_session_path, method: :delete %></li>
  </ul>
<% elsif admin_signed_in? %>
  <ul>
    <li>ユーザー一覧</li>
    <li><%= link_to '投稿一覧', posts_path %></li>
    <li>ジャンル一覧</li>
    <li><%= link_to 'ログアウト',destroy_admin_session_path, method: :delete %></li>
  </ul>
<% else %>
  <ul>
    <li><%= link_to 'Aboutページ', about_path %></li>
    <li><%= link_to '新規会員登録', new_member_registration_path %></li>
    <li><%= link_to 'ログイン', new_member_session_path %></li>
    <li><%= link_to 'スタッフログイン', new_admin_session_path %></li>
  </ul>
<% end %>