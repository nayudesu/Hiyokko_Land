<h1>みんなの投稿</h1>
  <% @posts.each do |posts| %>

<h6>ユーザーネーム</h6>
  <%= link_to public_post_path(member_id) do %>< %= posts.member.nickname %><% end %> 
  <!--パス確認必要-->

<h4>画像</h4>
  <% if posts.image.attached? %>
    <%= image_tag posts.image, size: "50x50" %>
  <% else %>
    <%= image_tag 'no_image', size: "50x50" %>
  <% end %>
  
<h4>ジャンル（未）</h4>

<h4>タイトル</h4>
<%= link_to public_post_path(posts) do %><%= posts.title %><% end %>

<% end %>